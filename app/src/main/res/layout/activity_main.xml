<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context=".VicMainActivity">


    <ImageView
        android:id="@+id/imageView"
        android:layout_width="330dp"
        android:layout_height="378dp"
        android:layout_marginBottom="432dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.567"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/musicplay2" />

    <ImageButton
        android:id="@+id/ButonTest"
        android:layout_width="104dp"
        android:layout_height="114dp"
        android:background="@android:color/transparent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.947"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:scaleType="fitCenter"
        app:layout_constraintVertical_bias="0.317"
        app:srcCompat="@drawable/vic_logo_d"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/bonhomme"
        android:layout_width="290dp"
        android:layout_height="513dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="103dp"
        android:layout_marginEnd="121dp"
        android:layout_marginBottom="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ButonTest"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/bonhomme2" />

    <ImageButton
        android:id="@+id/Button_Jacket"
        android:layout_width="167dp"
        android:layout_height="78dp"
        android:layout_marginBottom="32dp"
        android:background="@android:color/transparent"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toTopOf="@+id/Button_device"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/Button_device"
        app:srcCompat="@drawable/jacket"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/Button_device"
        android:layout_width="163dp"
        android:layout_height="59dp"
        android:layout_marginStart="59dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="228dp"
        android:background="@android:color/transparent"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.813"
        app:layout_constraintStart_toEndOf="@+id/bonhomme"
        app:srcCompat="@drawable/device"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/Button_roue"
        android:layout_width="84dp"
        android:layout_height="100dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="56dp"
        android:background="@android:color/transparent"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/bonhomme"
        app:layout_constraintTop_toBottomOf="@+id/Button_device"
        app:layout_constraintVertical_bias="0.298"
        app:srcCompat="@drawable/roue_para"
        tools:ignore="SpeakableTextPresentCheck" />


</androidx.constraintlayout.widget.ConstraintLayout>